{
    "name": "kevinvs/delegating-normalizer",
    "description": "A Normalizer for the symfony/serializer package, that allows you to define which specific normalizer/denormalizer should be used for a given property.",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kevinvs\\DelegatingNormalizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kevinvs\\DelegatingNormalizer\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Kevin van Sonsbeek",
            "homepage": "https://github.com/KevinVanSonsbeek"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "symfony/http-kernel": "^6.4",
        "symfony/dependency-injection": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    }
}
